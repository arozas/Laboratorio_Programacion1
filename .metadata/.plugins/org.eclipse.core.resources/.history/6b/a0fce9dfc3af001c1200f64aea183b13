#include <stdio.h>
#include <stdlib.h>

void divir(int numA, int numB);

int main(void)
{
	int num1 = 10;
	int num2 = 20;
	int resultado;

	resultado = divir(num1,num2);

	printf("la división es: %d", resultado);

	return EXIT_SUCCESS;
}
int divir(int numA, int numB)
{
	int rtn;

		rtn=numA/numB;

		return rtn;
}
