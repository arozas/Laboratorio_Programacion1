#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	short numeroIngresado;
	short contadorVueltas;
	int banderaPrimero;

	contadorVueltas=0;
	banderaPrimero=1;

	for(contadorVueltas = 0 ; contadorVueltas < 10 ; contadorVueltas++)
	{
		printf("Ingrese un número: \n");
		scanf("%hi", &numeroIngresado);


	}
	return EXIT_SUCCESS;
}
