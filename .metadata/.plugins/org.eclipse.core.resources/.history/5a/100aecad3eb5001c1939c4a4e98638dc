#include "Entrada.h"

int main(void)
{
	int numeroOficial;
	int retornoFuncion;

	retornoFuncion = getInt("\n Ingrese un numero: ", 3, 0, 10,"Error, te equivocaste", &numeroOficial);
	if(retornoFuncion==0)
	{
		printf("\n El numero es: %d", numeroOficial);
	}
	else
	{
		printf("\n Hubo un error");
	}

}
